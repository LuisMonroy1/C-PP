// Lect12_10.12_W8.2_DispArrayElements.cpp : This file contains the 'main' function. Program execution begins and ends there.
//

#include <iostream>
using namespace std;
int main() {
	int numbers[5] = { 7, 5, 6, 12, 35 };
	cout << "The numbers are: ";
	//  Printing array elements
	// using range based for loop
	for (const int& n : numbers) {
		cout << n << "  ";
	}


	cout << "\nThe numbers are: ";
	//  Printing array elements
	// using traditional for loop
	for (int i = 0; i < 5; ++i) {
		cout << numbers[i] << "  ";
	}
	return 0;
}
